{
  "servers": {
    "google-search": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@adenot/mcp-google-search"
      ],
      "env": {
        "GOOGLE_API_KEY": "${input:GOOGLE_API_KEY}",
        "GOOGLE_SEARCH_ENGINE_ID": "${input:GOOGLE_SEARCH_ENGINE_ID}"
      }
    },
    "io.github.upstash/context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@upstash/context7-mcp@1.0.31"
      ],
      "env": {
        "CONTEXT7_API_KEY": "${input:CONTEXT7_API_KEY}"
      },
      "gallery": "https://api.mcp.github.com",
      "version": "1.0.31"
    },
    "gcapnias/feature-mcp-server": {
      "type": "stdio",
      "command": "pnpm",
      "args": [
        "--filter",
        "@gcapnias/tasks-mcp-server",
        "start"
      ],
      "cwd": "./apps/tasks-mcp-server"
    }
  },
  "inputs": [
    {
      "id": "CONTEXT7_API_KEY",
      "type": "promptString",
      "description": "API key for authentication",
      "password": true
    },
    {
      "id": "GOOGLE_API_KEY",
      "type": "promptString",
      "description": "Google API key, for authentication",
      "password": true
    },
    {
      "id": "GOOGLE_SEARCH_ENGINE_ID",
      "type": "promptString",
      "description": "Google Search Engine ID, for authentication",
      "password": true
    }
  ]
}